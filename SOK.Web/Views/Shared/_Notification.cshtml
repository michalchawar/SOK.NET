@if (TempData["error"] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript" >
        $(document).ready(() => {
            notifications.error('@TempData["error"]');
        });
    </script>
}
